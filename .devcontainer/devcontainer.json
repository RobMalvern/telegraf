{
	"name": "Go",
	"dockerFile": "Dockerfile",
	"appPort": 9000,
	"workspaceMount": "src=c:/Dev/github/telegraf,dst=/workspaces/telegraf",
	"workspaceFolder": "/workspaces/telegraf",
	"extensions": [
		"ms-vscode.go"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"go.gopath": "/go",
		"go.inferGopath": true,
		"go.useLanguageServer": true
	}
}