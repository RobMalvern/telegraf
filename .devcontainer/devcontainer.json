{
	"name": "Go",
	"dockerFile": "Dockerfile",
	"appPort": 9000,
	"workspaceMount": "src=${localWorkspaceFolder},dst=/go/src/github.com/influxdata/telegraf,type=bind,consistency=cached",
	"workspaceFolder": "/go/src/github.com/influxdata/telegraf",
	"extensions": [
		"ms-vscode.go"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"settings": {
		"go.gopath": "/go",
		"go.inferGopath": true,
		"go.useLanguageServer": true
	}
}